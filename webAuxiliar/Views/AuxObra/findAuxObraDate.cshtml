@{
    ViewBag.Title = "Información Auxiliar Obras";
}
<br />

<!DOCTYPE html>
<html lang="es-ec">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    @*<meta name="viewport" content="width=device-width, initial-scale=1">*@
    @*<link rel="stylesheet" href="~/Content/font-awesome-4.7.0/css/font-awesome.css">*@
    
    <script type="text/ecmascript" src="~/Scripts/jquery-2.1.1.min.js"></script>
    <script type="text/ecmascript" src="~/Scripts/jqGridTrirand/jquery.jqGrid.js"></script>
    
    <script type="text/ecmascript" src="~/Scripts/jqGridTrirand/i18n/grid.locale-es.js"></script>
    <script src="~/Scripts/jquery-ui-1.12.1.js"></script>
    <script src="~/Scripts/pdfmake0.1.33/pdfmake.min.js"></script>
    <script src="~/Scripts/pdfmake0.1.33/vfs_fonts.js"></script>
    <script type="text/javascript" language="javascript" src="~/Scripts/jszip3.1.4-22/jszip.min.js"></script>
    <link rel="stylesheet" type="text/css" media="screen" href="~/Content/themes/base/jquery-ui.css">
    <link rel="stylesheet" type="text/css" media="screen" href="~/Scripts/jqGridTrirand/css/ui.jqgrid.css">

</head>

<body>
    <fieldset>
        <div>
            <form>
                <table style="width:30%">
                    <tr>
                        <th>Fecha Desde</th>
                        <th>Fecha Hasta</th>
                        <th></th>
                    </tr>
                    <tr>
                        <td>@Html.TextBox("txtFechaDesde", string.Empty, new { @class = "datepicker", @type = "date" })</td>
                        <td>@Html.TextBox("txtFechaHasta", string.Empty, new { @class = "datepicker", @type = "date" })</td>
                        <td><input class="btn btn-primary" type="submit" value="Buscar" /></td>
                    </tr>
                </table>
            </form>
            <br />
            <table id="jqGridAux"></table>
            <div id="jqGridAuxPag"></div>
            <br />
            @*<button id="exportar">Exportar a Excel</button>*@
            <input type="button" value="Fila Seleccionada" onclick="getSelectRows()" />

        </div>
    </fieldset>
</body>
</html>



