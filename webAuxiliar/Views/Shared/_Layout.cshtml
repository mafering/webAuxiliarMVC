<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @*<title>@ViewBag.Title - Auxiliares IMP</title>*@
    <title>@ViewBag.Title</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @*@Html.ActionLink("Auxiliares IMP", "findAuxObraDate", "AuxObra", new { area = "" }, new { @class = "navbar-brand" })*@
                @Html.ActionLink("Auxiliares IMP", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Obras", "findAuxObraDate", "AuxObra")</li>
                    <li>@Html.ActionLink("Servicios", "findAuxServDate", "AuxServicio")</li>
                    <li>@Html.ActionLink("REPORTES", "Index", "Report")</li>
                </ul>
                
            </div>
        </div>
    </div>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>Auxiliares IMP WebMVC Application - &copy; @DateTime.Now.Year</p>
        </footer>
    </div>

    @RenderSection("scripts")
</body>
</html>